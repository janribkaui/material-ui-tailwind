{
  "name": "@janribkaui/styled-engine",
  "version": "1.5.1",
  "private": false,
  "author": "Jan Ribka",
  "description": "styled() API wrapper package for emotion.",
  "license": "MIT",
  "main": "./src/index.ts",
  "keywords": [
    "react",
    "react-components",
    "janribka",
    "janribkaui",
    "styled-engine"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/janribkaui/material-ui-tailwind.git",
    "directory": "packages/janribkaui-styled-engine"
  },
  "bugs": {
    "url": "https://github.com/janribkaui/material-ui-tailwind/issues"
  },
  "homepage": "Private package",
  "scripts": {
    "build": "pnpm build:modern && pnpm build:node && pnpm build:stable && pnpm build:copy-files",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:node": "node ../../scripts/build.mjs node",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "prebuild": "rimraf build",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "release": "pnpm build && pnpm version --no-git && pnpm publish --access public",
    "typescript": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.6",
    "@emotion/cache": "11.13.1",
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@janribkaui/styled-engine": "workspace:*",
    "@types/chai": "^4.3.17",
    "@types/react": "catalog:",
    "chai": "^5.1.1",
    "react": "catalog:",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@types/react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
